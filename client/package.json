{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "npm-run-all -p dev tailwind:watch",
    "tailwind:watch": "tailwindcss -i ./src/ui/css/index.css -o ./src/ui/css/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./src/ui/css/index.css -o ./src/ui/css/tailwind.css --minify",
